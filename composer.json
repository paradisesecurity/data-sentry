{
    "name": "paradisesecurity/data-sentry",
    "type": "library",
    "description": "Database encryption in PHP applications.",
    "homepage": "https://paradisesecurity.work",
    "license": "MIT",
    "authors": [
        {
            "name": "Paradise Security",
            "email": "joe@paradisesecurity.work",
            "homepage": "https://paradisesecurity.work",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/collections": "^2.0",
        "paradisesecurity/service-registry": "^0.1 || dev-master",
        "paragonie/ciphersweet": "^4.0",
        "symfony/cache": "^5.4|^6.0",
        "symfony/property-access": "^5.4|^6.0",
        "symfony/serializer": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ParadiseSecurity\\Component\\DataSentry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "ParadiseSecurity\\Component\\DataSentry\\Test\\": "tests/Test/" }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
